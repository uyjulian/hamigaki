using testing ;

project
    : requirements
      <library>/boost/test//boost_unit_test_framework/<link>static
    ;

test-suite "checksum" :
    [ run md5_test.cpp : ]
    [ run sha1_test.cpp : ]
    [ run sum8_test.cpp : ]
    [ run xor8_test.cpp : ]
    ;
