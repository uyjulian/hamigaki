<header name="hamigaki/bjam/util/module.hpp">
  <namespace name="hamigaki">
    <namespace name="bjam">
      <struct name="module">
        <data-member name="rules">
          <type><classname>rule_table</classname></type>
        </data-member>

        <data-member name="variables">
          <type><classname>variable_table</classname></type>
        </data-member>

        <data-member name="class_module">
          <type>boost::optional&lt;std::string&gt;</type>
        </data-member>

        <data-member name="imported_modules">
          <type>std::set&lt;std::string&gt;</type>
        </data-member>

        <data-member name="native_rules">
          <type>std::map&lt;std::string,<classname>native_rule</classname>&gt;</type>
        </data-member>
      </struct>
    </namespace>
  </namespace>
</header>
