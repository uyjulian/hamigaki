<header name="hamigaki/archivers/cpio/file_format.hpp">
  <namespace name="hamigaki">
    <namespace name="archivers">
      <namespace name="cpio">
        <enum name="file_format">
          <enumvalue name="posix" />
          <enumvalue name="svr4" />
          <enumvalue name="svr4_chksum" />
          <enumvalue name="binary" />
          <purpose><simpara>cpioファイルのフォーマット</simpara></purpose>
          <description>
            <informaltable frame="all">
              <tgroup cols="2">
                <thead>
                  <row>
                    <entry>値</entry>
                    <entry>意味</entry>
                  </row>
                </thead>
                <tbody>
                  <row>
                    <entry>posix</entry>
                    <entry>POSIX.1-1998 cpio フォーマット</entry>
                  </row>
                  <row>
                    <entry>svr4</entry>
                    <entry>SVR4 フォーマット</entry>
                  </row>
                  <row>
                    <entry>svr4_chksum</entry>
                    <entry>チェックサム付きSVR4 フォーマット</entry>
                  </row>
                  <row>
                    <entry>binary</entry>
                    <entry>GNU cpio互換 バイナリーフォーマット</entry>
                  </row>
                </tbody>
              </tgroup>
            </informaltable>
          </description>
        </enum>
      </namespace>
    </namespace>
  </namespace>
</header>
